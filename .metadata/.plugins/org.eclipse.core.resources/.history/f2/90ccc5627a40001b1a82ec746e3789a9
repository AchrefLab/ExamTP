package firstpack;

public class Role {
	private String lerole;
	public Role(String unrole) {
		this.lerole=unrole;
	}
	public boolean equals(Object thisobject) {
		if (this == thisobject) {
			return true;
		}else { return false;}
	}
	public int hashCode() {
		return Objects.hash(nomRole);
	}
	public String nomRole;
	
	
	private String nomRole;


	public boolean equals(Object o) {
		    if (this == o) return true;
		    Role role = (Role) o;
		    return Objects.equals(this.nomRole, role.nomRole);
	}
	 public int hashCode() {
	        return Objects.hash(nomRole);
	    }
	 public Role(String nomRole) {
		 this.nomRole=nomRole;
	 }
}
